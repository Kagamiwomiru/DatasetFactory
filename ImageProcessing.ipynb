    "def LensFilter(img,color,scale):\n",
    "    b,g,r=img[:,:,0],img[:,:,1],img[:,:,2]\n",
    "    if(color==\"red\"):\n",
    "        img[:,:,0]=0\n",
    "        img[:,:,1]=0\n",
    "        img[:,:,2]=r//scale\n",
    "    elif(color==\"green\"):\n",
    "        img[:,:,0]=0\n",
    "        img[:,:,1]=g//scale\n",
    "        img[:,:,2]=0\n",
    "    elif(color==\"blue\"):\n",
    "        img[:,:,0]=b//scale\n",
    "        img[:,:,1]=0\n",
    "        img[:,:,2]=0\n",
    "    \n",
    "    return img"
