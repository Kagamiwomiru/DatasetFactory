    "def ModChroma(img,chroma):\n",
    "    a=cv2.cvtColor(img,cv2.COLOR_BGR2HSV)\n",
    "    hsv=cv2.split(a)\n",
    "    hsv[1]=hsv[1]//chroma\n",
    "    img=cv2.merge((hsv[0],hsv[1],hsv[2]))\n",
    "    img=cv2.cvtColor(img,cv2.COLOR_HSV2BGR)\n",
    "    return img\n",
    "    "
